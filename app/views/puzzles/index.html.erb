<p id="notice"><%= notice %></p>

<h1 class="text-4xl mb-8">Puzzles</h1>

    <% @puzzles.each do |puzzle| %>
      <div class="mt-8">
      <span class="p-4 border border-black">
        <%= link_to puzzle.title, puzzle  %>
      </span>
      </div>
    <% end %>


<table>
  <tbody>
    <% @puzzles.each do |puzzle| %>
      <tr>
        <td class="pr-4"><%= link_to puzzle.title, puzzle  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

